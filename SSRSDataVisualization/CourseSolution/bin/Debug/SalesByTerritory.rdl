  </DataSources>
  <DataSets>
    <DataSet Name="dsSalesTerritory">
      <Query>
        <DataSourceName>srcSalesData</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@SalesYear">
            <Value>=Parameters!SalesYear.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT	SUM(so.SalesAmount) AS SalesAmount
		,YEAR(so.SalesDate) AS SalesYear,
		st.TerritoryName 
FROM Sales.SalesOrder so
JOIN Sales.SalesTerritory st ON so.SalesTerritory = st.Id
WHERE YEAR(so.SalesDate) IN (@SalesYear)
GROUP BY YEAR(so.SalesDate), st.TerritoryName </CommandText>
      </Query>
      <Fields>
        <Field Name="SalesAmount">
          <DataField>SalesAmount</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="SalesYear">
          <DataField>SalesYear</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="TerritoryName">
          <DataField>TerritoryName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsSalesYear">
      <Query>
        <DataSourceName>srcSalesData</DataSourceName>
        <CommandText>SELECT DISTINCT YEAR(SalesDate) AS SalesYear
FROM Sales.SalesOrder 
ORDER BY YEAR(SalesDate);</CommandText>
      </Query>
      <Fields>
        <Field Name="SalesYear">
          <DataField>SalesYear</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
